{
  "name": "opencontribute",
  "version": "1.0.0",
  "description": "A connection point for those who have a vision but no resources for their project, and for those who have those resources and are willing to share/commit them to a common project.",
  "main": "server.js",
  "scripts": {
    "test": "nodemon server.js",
    "start": "node server.js",
    "debug": "ndb server.js",
    "build": "parcel build"
  },
  "targets": {
    "modern": {
      "source": "public/static/js/*.js",
      "distDir": "./public/static/js/modern",
      "context": "browser",
      "engines": {
        "browsers": "Chrome 131"
      }
    },
    "legacy": {
      "source": "public/static/js/*.js",
      "distDir": "./public/static/js/legacy",
      "context": "browser",
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    },
    "styles": {
      "source": "public/static/css/*.css",
      "distDir": "./public/static/css/mini",
      "context": "browser",
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    }
  },
  "keywords": [
    "common",
    "cause",
    "project",
    "resources",
    "help",
    "vision"
  ],
  "author": "Peter Duricek",
  "license": "ISC",
  "dependencies": {
    "@dotenvx/dotenvx": "^1.5.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.41.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "html-to-text": "^9.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.5",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.14",
    "parcel": "^2.14.1",
    "path": "^0.12.7",
    "pug": "^3.0.3",
    "regenerator-runtime": "^0.14.1",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "stripe": "^17.4.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "ndb": "^1.1.5",
    "nodemon": "^3.1.7"
  }
}
